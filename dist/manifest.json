{
    "name": "current tabs",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_title": "current tabs"
    },
    "background": {
        "service_worker": "background.js"
    },
    
    "permissions": [
        "activeTab",
        "tabs"
    ]
}